# Secret used to sign JWT tokens
JWT_SECRET=your_jwt_secret_key


# ===============================
# üìß EMAIL SERVICE (GMAIL SMTP)
# ===============================
# Gmail App Password (not your regular password)
EMAIL_USER=ur_email
EMAIL_PASS=ur_app_password


# ===============================
